{
  "name": "anport",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node scripts/organize-sourcemaps.js",
    "preview": "vite preview",
    "lint": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:regression": "playwright test --grep @regression",
    "test:e2e:regression:trace": "playwright test tests/e2e/regression-trace.spec.ts",
    "test:e2e:full": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "check": "npm run lint && npm run test:ci && npm run test:coverage && npm run build",
    "push:maintenance": "node scripts/send-maintenance-push.js"
  },
  "dependencies": {
    "firebase": "^12.9.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "jsdom": "^25.0.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^2.1.9"
  }
}
